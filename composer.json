{
    "name": "realodix/dessert",
    "description": "Realodix dessert",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/realodix/next-project",
    "authors": [{"name": "Budi Hermawan", "role": "Developer" }],
    "require": {
        "php": "^8.0",
        "phpunit/phpunit": "^9.6 || ^10.0",
        "symfony/polyfill-php80": "^1.26",
        "symfony/polyfill-php81": "^1.26"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "realodix/relax": "dev-main"
    },
    "autoload": {
        "files": [
            "src/Functions.php"
        ],
        "psr-4": {
            "Realodix\\Dessert\\": ["src", "src/Dessert"]
        }
    },
    "autoload-dev": {
        "files": [
            "tests/bootstrap.php"
        ],
        "psr-4": {
            "Realodix\\Dessert\\Test\\": ["tests", "tests/Unit"]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "./vendor/bin/php-cs-fixer fix",
        "test": "./vendor/bin/phpunit"
    }
}
