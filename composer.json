{
    "name": "realodix/dessert",
    "description": "Realodix dessert",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/realodix/next-project",
    "authors": [
        {
            "name": "Budi Hermawan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "phpunit/phpunit": "^8.5|^9.5",
        "symfony/css-selector": "^5.4|^6",
        "symfony/dom-crawler": "^5.4|^6",
        "symfony/polyfill-php80": "^1.26",
        "symfony/polyfill-php81": "^1.26"
    },
    "require-dev": {
        "realodix/cs-config": "1.0.x-dev|dev-main"
    },
    "autoload": {
        "files": [
            "src/Functions.php"
        ],
        "psr-4": {
            "Realodix\\Dessert\\": ["src", "src/Dessert"]
        }
    },
    "autoload-dev": {
        "files": [
            "tests/bootstrap.php"
        ],
        "psr-4": {
            "Realodix\\Dessert\\Test\\": ["tests", "tests/Unit"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "./vendor/bin/php-cs-fixer fix",
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
